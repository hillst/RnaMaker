{%  extends "HillCMSRnaMakerBundle:Default:syntasi-template.html.twig" %}
{% block body %}
<div class="container">
	<div class="jumbotron results" style="float:none">
		<h1 class="tool-header"> SyntasiRNA Oligo Results </h1>
        <div>
        <div class ="col-lg-3">syntasiRNA Cassette: </div><div class="col-lg-9">5' {{ amiRNA }} 3'</div>
        {% for syntasi in syntasis %}
            {# syntasi[0] is assigned name, syntasi[1] is the syntasiRNA, syntasi[2] is the star strand #}
            <div class="col-lg-3">{{ syntasi[0] }}*:</div><div class="col-lg-9">5' {{syntasi[2] }} 3'</div>
            <div class="col-lg-3">{{ syntasi[0] }}:</div><div class="col-lg-9">5' {{ syntasi[1] }} 3'</div> 
        {% endfor %}
        <div class ="col-lg-3">Forward Oligo: </div><div class="col-lg-9">5' {{ oligo1 }} 3'</div>
        <div class ="col-lg-3">Reverse Oligo: </div><div class="col-lg-9">5' {{ oligo2 }} 3'</div>
        </div>
        <a id = "download" class = "btn btn-default btn-block btn-tertiary" href="{{ asset(dl_token)}}" style="display:inline-block" download>Download</a>
	</div>	
	
</div>
{% endblock %}
